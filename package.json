    "@oracle/ojet-cli": "^13.0.0",
    "@oracle/oraclejet-tooling": "^13.0.1",
    "@prefresh/babel-plugin": "0.4.3",
    "@prefresh/webpack": "3.3.4",
    "babel-loader": "^8.2.5",
    "compression-webpack-plugin": "10.0.0",
