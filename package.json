    "css-loader": "6.7.1",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.1",
    "html-replace-webpack-plugin": "2.6.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "noop-loader": "1.0.0",
    "raw-loader": "4.0.2",
    "style-loader": "3.3.1",
